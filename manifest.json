{
  "name": "Dark Octo Themes",
  "version": "1.3",
  "manifest_version": 2,
  "description": "Apply different source code themes when viewing github repositories",
  "background": {
    "scripts": [
        "applyTheme.js"       
    ]
  },

  "background" : { "scripts": [ "applyTheme.js" ]  },
  "permissions": [ 
    "tabs", 
    "https://gist.github.com/*",
    "https://github.com/*"
  ]
}
