{
  "name": "Dark Octo Themes",
  "version": "1.14",
  "manifest_version": 2,
  "description": "Allows you to view source code on GitHub with a dark theme instead of the default light theme.",
  "icons": {
    "128": "src/images/icon.png"
  },
  "background": {
    "scripts": ["src/js/applyTheme.js", "src/js/themeStorage.js"]
  },
  "permissions": ["tabs", "https://gist.github.com/*", "https://github.com/*"],
  "options_page": "src/options.html"
}
