{
  "name": "Dark Octo Themes",
  "version": "1.8",
  "manifest_version": 2,
  "description": "Apply different source code themes when viewing github repositories",
  "background": {
    "scripts": [
        "src/js/applyTheme.js",
        "src/js/themeStorage.js"
    ]
  },
  "permissions": [ 
    "tabs", 
    "https://gist.github.com/*",
    "https://github.com/*"
  ],
  "options_page": "src/options.html"
}
