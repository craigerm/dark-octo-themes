{
  "name": "Dark Octo Themes",
  "version": "1.4",
  "manifest_version": 2,
  "description": "Apply different source code themes when viewing github repositories",
  "background": {
    "scripts": [
        "schemeManager.js",     
        "applyTheme.js"       
    ]
  },
  "permissions": [ 
    "tabs", 
    "https://gist.github.com/*",
    "https://github.com/*"
  ],
  "options_page": "options.html"

}
